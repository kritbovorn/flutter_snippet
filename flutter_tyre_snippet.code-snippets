		  "  set value(int newValue) {",
		  "    if (newValue != _value) {",
		  "      _value = newValue;",
		  "      notifyListeners();",
		  "    }",
		  "  }",
		  "",
		  "  void increment() {",
		  "    _value += 1;",
		  "    notifyListeners();",
		  "  }",
		  "}",
